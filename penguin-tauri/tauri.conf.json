{
  "build": {
    "beforeDevCommand": "npm run --prefix svelte-ui dev",
    "beforeBuildCommand": "npm run --prefix svelte-ui build",
    "devPath": "http://localhost:5173/",
    "distDir": "../svelte-ui/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "penguin",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.penguin.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 708,
        "resizable": true,
        "title": "penguin",
        "width": 1120
      }
    ]
  }
}